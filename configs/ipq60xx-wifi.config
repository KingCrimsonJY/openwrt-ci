# ==========================================
# 1. 目标设备设定 (Target)
# ==========================================
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y

# ==========================================
# 1.1 固件输出格式设定 (确保生成 factory 固件)
# ==========================================
# 使用 SquashFS 文件系统 (默认，factory/sysupgrade 都需要)
CONFIG_TARGET_ROOTFS_SQUASHFS=y
# 禁用 initramfs (减少编译时间，刷机不需要)
# CONFIG_TARGET_ROOTFS_INITRAMFS is not set
# 不需要 ext4 和 cpiogz 格式
# CONFIG_TARGET_ROOTFS_EXT4FS is not set
# CONFIG_TARGET_ROOTFS_CPIOGZ is not set

# ==========================================
# 2. 系统核心与基础库 (Base & Libs)
# ==========================================
# LuCI 基础和完整管理界面
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-compat=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-mod-network=y
CONFIG_PACKAGE_luci-mod-status=y
CONFIG_PACKAGE_luci-mod-system=y

# 中文语言包 (简体中文)
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_PACKAGE_luci-i18n-base-zh-cn=y
CONFIG_PACKAGE_luci-i18n-firewall-zh-cn=y

# 核心依赖库
CONFIG_PACKAGE_libstdcpp=y
CONFIG_PACKAGE_liblucihttp=y
CONFIG_PACKAGE_liblucihttp-ucode=y
CONFIG_PACKAGE_cgi-io=y
# UPnP/NatPMP 库
CONFIG_PACKAGE_libminiupnpc=y
CONFIG_PACKAGE_libnatpmp=y
# RPCD (系统调用)
CONFIG_PACKAGE_rpcd=y
CONFIG_PACKAGE_rpcd-mod-file=y
CONFIG_PACKAGE_rpcd-mod-luci=y
CONFIG_PACKAGE_rpcd-mod-ucode=y
# ucode 模块
CONFIG_PACKAGE_ucode-mod-html=y
CONFIG_PACKAGE_ucode-mod-log=y
CONFIG_PACKAGE_ucode-mod-math=y


# ==========================================
# 3. 内核驱动：USB 支持 (Kernel - USB)
# ==========================================
# USB 核心与版本支持
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-usb3=y
# USB 控制器驱动 (Qualcomm DWC3)
CONFIG_PACKAGE_kmod-usb-dwc3=y
CONFIG_PACKAGE_kmod-usb-dwc3-qcom=y
# 老旧设备兼容 (OHCI/UHCI)
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb-uhci=y

# ==========================================
# 4. 内核驱动：存储与文件系统 (Kernel - Storage)
# ==========================================
# USB 存储协议
CONFIG_PACKAGE_kmod-usb-storage=y
CONFIG_PACKAGE_kmod-usb-storage-extras=y
CONFIG_PACKAGE_kmod-usb-storage-uas=y
# SCSI 协议 (兼容性增强)
CONFIG_PACKAGE_kmod-scsi-core=y
CONFIG_PACKAGE_kmod-scsi-generic=y
# 文件系统支持
CONFIG_PACKAGE_kmod-fs-ext4=y
CONFIG_PACKAGE_kmod-fs-vfat=y
CONFIG_PACKAGE_kmod-fs-exfat=y
CONFIG_PACKAGE_kmod-fs-ntfs3=y

# ==========================================
# 5. 内核驱动：网络与安卓连接 (Kernel - Network)
# ==========================================
# 虚拟网卡与 FUSE
CONFIG_PACKAGE_kmod-tun=y
CONFIG_PACKAGE_kmod-fuse=y
# 安卓 USB 网络共享 (RNDIS/CDC)
CONFIG_PACKAGE_kmod-usb-net=y
CONFIG_PACKAGE_kmod-usb-net-rndis=y
CONFIG_PACKAGE_kmod-usb-net-cdc-ether=y

# ==========================================
# 6. 系统工具与命令行 (Utilities)
# ==========================================
# 磁盘管理与挂载
CONFIG_PACKAGE_fdisk=y
CONFIG_PACKAGE_lsblk=y
CONFIG_PACKAGE_block-mount=y
CONFIG_PACKAGE_automount=y
CONFIG_PACKAGE_hdparm=y
# 网络与系统监控
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_ip-tiny=y
# 安卓 MTP 工具
CONFIG_PACKAGE_libmtp=y
CONFIG_PACKAGE_mtp-tools=y
CONFIG_PACKAGE_jmtpfs=y
# 加密库 (VPN依赖)
CONFIG_PACKAGE_libgnutls=y
CONFIG_PACKAGE_libxml2=y

# ==========================================
# 7. VPN 客户端支持
# ==========================================
# Cisco AnyConnect (OpenConnect)
CONFIG_PACKAGE_openconnect=y
CONFIG_PACKAGE_luci-proto-openconnect=y
CONFIG_PACKAGE_vpnc-scripts=y

# ==========================================
# 8. LuCI 应用软件 (Applications)
# ==========================================
# 存储与共享
CONFIG_PACKAGE_luci-app-diskman=y
CONFIG_PACKAGE_luci-i18n-diskman-zh-cn=y
CONFIG_PACKAGE_luci-app-hd-idle=y
CONFIG_PACKAGE_luci-i18n-hd-idle-zh-cn=y
CONFIG_PACKAGE_luci-app-samba4=y
CONFIG_PACKAGE_luci-i18n-samba4-zh-cn=y
CONFIG_PACKAGE_luci-app-openlist2=y
# 网络服务与穿透
CONFIG_PACKAGE_luci-app-upnp=y
CONFIG_PACKAGE_luci-i18n-upnp-zh-cn=y
CONFIG_PACKAGE_luci-app-tailscale-community=y
# 系统管理与监控
CONFIG_PACKAGE_luci-app-package-manager=y
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-i18n-ttyd-zh-cn=y
CONFIG_PACKAGE_luci-app-nlbwmon=y
CONFIG_PACKAGE_luci-i18n-nlbwmon-zh-cn=y


# ==========================================
# 9. WiFi 支持 (Wireless - IPQ60xx WiFi 6)
# ==========================================
CONFIG_PACKAGE_kmod-qca-mcs=y
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=y
CONFIG_PACKAGE_kmod-qca-nss-drv-lag-mgr=y
CONFIG_PACKAGE_kmod-qca-nss-drv-map-t=y
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=y
CONFIG_PACKAGE_kmod-qca-nss-drv-qdisc=y
CONFIG_PACKAGE_kmod-qca-nss-macsec=y
CONFIG_NSS_DRV_IGS_ENABLE=y
CONFIG_NSS_DRV_L2TP_ENABLE=y
CONFIG_NSS_DRV_LAG_ENABLE=y
CONFIG_NSS_DRV_MAPT_ENABLE=y
CONFIG_NSS_DRV_PPTP_ENABLE=y
CONFIG_NSS_DRV_PVXLAN_ENABLE=y
CONFIG_NSS_DRV_SHAPER_ENABLE=y
CONFIG_NSS_DRV_VXLAN_ENABLE=y
CONFIG_NSS_FIRMWARE_VERSION_12_2=y
# ath11k 驱动 (Qualcomm WiFi 6)
CONFIG_PACKAGE_kmod-ath11k=y
CONFIG_PACKAGE_kmod-ath11k-ahb=y
CONFIG_PACKAGE_ath11k-firmware-ipq6018=y
# 无线管理工具
CONFIG_PACKAGE_wpad-openssl=y
CONFIG_PACKAGE_hostapd-common=y
CONFIG_PACKAGE_wireless-regdb=y
CONFIG_PACKAGE_iw=y

# ==========================================
# 10. 主题与界面 (Themes)
# ==========================================
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y